{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "plugins": [
    'import',
    'jsx-a11y',
    'react'
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "jsx-a11y/href-no-hash": 0,
    "react/prefer-stateless-function": 0,
    "semi": 0
  }
}